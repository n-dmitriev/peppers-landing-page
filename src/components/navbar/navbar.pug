mixin navbar(currentPage)
    nav.navbar
        .navbar__menu
            .navbar__links
                ul
                  each val in menuItems
                    +renderList(val,currentPage)
            .navbar__container
                button.btn Raffeisen-Online
                button.btn_light R-Dealler
        .navbar__footer
            .navbar__donwoald
                .pdf-icon
                button.btn-donwoald Скачать "услуги PWR"
            .navbar_about
                2009-20017

//- 
mixin renderList(val,currentPage)
    -if(val.isSeparator === true)
        li #[div.h-line]
    -else
        li(class=(val.id==currentPage ? "active-item" : ""))= val.title